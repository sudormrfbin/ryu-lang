?start: term

?!term: unary_op ("+" | "-" | "*" | "/" | "%" | "&&") term
	 | unary_op

?!unary_op: ("+" | "-") unary_op
		  | atom

?atom: bool_literal | int_literal | string_literal

bool_literal: TRUE | FALSE
TRUE: "true"
FALSE: "false"

%import common.INT
int_literal: INT

string_literal: STRING
STRING: "\"" /.*?/ "\""
